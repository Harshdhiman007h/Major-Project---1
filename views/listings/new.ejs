<% layout("/layouts/boilerplate") %>
<body>
    <h2>Create New Listing</h2>
    <form method="POST" action="/listings">
        <input name="listing[title]" placeholder="enter title">
        <br><br>
        <input name="listing[description]" placeholder="enter description">
        <br><br>
        <input name="listing[image]" placeholder="enter image">
        <br><br>
        <input name="listing[price]" placeholder="enter price">
        <br><br>
        <input name="listing[location]" placeholder="enter location">
        <br><br>
        <input name="listing[country]" placeholder="enter country">
        <br><br>
        <button>Add</button>
    </form>
</body>